{
    "name": "slim/slim-skeleton",
    "description": "This is a simple CSV file uploader built on top of the Slim PHP framework and the Slim-Skeleton.",
    "keywords": ["csv", "upload", "import", "application"],
    "homepage": "https://github.com/keganv/csv-uploader",
    "license": "MIT",
    "authors": [
        {
            "name": "Kegan VanSickle",
            "email": "keganv@keganv.com",
            "homepage": "http://www.keganv.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "doctrine/orm": "^2.6",
        "league/csv": "^9.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload": {
        "psr-4" : {
            "" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
